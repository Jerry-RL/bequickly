{
  "name": "bequickly",
  "version": "1.0.0",
  "description": "A CLI tool for quickly generating projects from templates",
  "main": "dist/index.js",
  "bin": {
    "bequickly": "./dist/cli.js",
    "bq": "./dist/cli.js",
    "bequicklyproject": "./dist/cli.js",
    "bqp": "./dist/cli.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "link": "npm run build && npm link",
    "unlink": "npm unlink -g bequickly",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "template",
    "generator",
    "scaffold"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "templates"
  ]
}

